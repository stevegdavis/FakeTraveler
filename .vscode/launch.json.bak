	"version": "0.2.0",
	"configurations": [
		{
			"type": "firefox",
			"request": "launch",
			"reAttach": true,
			"name": "Launch FF",
			"url": "http://192.168.1.125:4000/triplogger-server/index.php",
			"webRoot": "C:/SRC/Web/Servers/triplogger-server"
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome",
			"url": "http://192.168.1.125:4000/triplogger-server/index.php",
			"webRoot": "C:/SRC/Web/Servers/triplogger-server"
		},
		{
			"name": "Listen for XDebug",
			"type": "php",
			"request": "launch",
			"port": 9003
		},
		{
			"name": "Launch currently open script",
			"type": "chrome",
			"request": "launch",
			"program": "${file}",
			"cwd": "${fileDirname}",
			"port": 9000
		}
	]
}